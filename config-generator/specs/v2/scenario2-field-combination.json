{
  "openapi": "3.0.0",
  "info": {
    "title": "Insured API V2",
    "version": "2.0.0"
  },
  "paths": {
    "/api/v2/insured/{customerId}": {
      "get": {
        "operationId": "getInsured",
        "parameters": [
          {
            "name": "customerId",
            "in": "path",
            "required": true,
            "schema": {"type": "string"}
          }
        ],
        "responses": {
          "200": {
            "description": "Insured person details",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "fullName": {
                      "type": "string",
                      "description": "Full name of insured person"
                    },
                    "age": {"type": "integer"},
                    "email": {"type": "string"}
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}